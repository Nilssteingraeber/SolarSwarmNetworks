{
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": ["vite/client"],
    "noEmit": true,
  },
  "exclude": [
    "node_modules",
    "dist",
    "coverage"
  ]
}